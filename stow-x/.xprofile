#!/bin/sh

xrandr --output LVDS1 --auto --primary \
	--output DP1 --auto --right-of LVDS1 \

xrdb -merge ~/.Xresources
xsetroot -cursor_name left_ptr
feh --bg-scale ~/Arch_Linux_Wallpaper.jpg
urxvtd -q -f -o
setxkbmap -option "" -layout "us,se,dvp" -option ctrl:swapcaps
killall pwld
pwld -l us,se,dvp -d
xmodmap ~/.xmodmaprc

